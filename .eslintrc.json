{
    "parser": "babel-eslint",
    "extends": ["airbnb"],
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "ecmaFeatures": {
        "jsx": true,
        "modules": true,
        "classes": true
    },
    "rules": {
        "indent": ["error", 4]
    },
    "plugins":[
        "react"
    ]
}
